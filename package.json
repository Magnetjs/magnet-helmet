{
  "name": "magnet-helmet",
  "version": "0.1.6",
  "description": "Readme",
  "keywords": [
    "helmet",
    "magnet"
  ],
  "homepage": "https://github.com/kievechua/magnet_modules#readme",
  "bugs": {
    "url": "https://github.com/kievechua/magnet_modules/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com>",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kievechua/magnet_modules.git"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "start": "babel src --out-dir dist",
    "test": "babel-node tests/index.js | tap-spec",
    "watch": "nodemon --ignore dist --exec \"babel src --out-dir dist --source-maps\"",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "nodemon --delay 0.5 --ignore src --exec babel-node tests/index.js | tap-spec"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint-config-airbnb",
    "globals": {
      "__DEVELOPMENT__": true,
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DISABLE_SSR__": true,
      "__DEVTOOLS__": true,
      "socket": true,
      "webpackIsomorphicTools": true
    },
    "plugins": [
      "import"
    ],
    "rules": {
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "react/jsx-quotes": 0,
      "react/no-multi-comp": 0,
      "import/default": 0,
      "import/no-duplicates": 0,
      "import/named": 0,
      "import/namespace": 0,
      "import/no-unresolved": 0,
      "import/no-named-as-default": 2,
      "jsx-quotes": 2,
      "block-scoped-var": 0,
      "padded-blocks": 0,
      "comma-dangle": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "no-console": 0,
      "no-alert": 0
    },
    "settings": {
      "import/parser": "babel-eslint",
      "import/resolve": {
        "moduleDirectory": [
          "node_modules",
          "src"
        ]
      }
    }
  },
  "dependencies": {
    "koa-helmet": "^2.0.0-alpha.1",
    "magnet-core": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "nodemon": "^1.9.1",
    "tap-spec": "^4.1.1"
  }
}
